var d = database("org.h2.Driver", "jdbc:h2:/tmp/test", "admin", "admin")
if (database_connected(d)) {
    var q = [ 
        ["insert into test(a,b) values(?, ?)", [random(1,10), "hello"]], 
        ["update test set b=? where b=?", ["Hello World", "hello"]] 
    ]
    var r = query(d, q)
    println(r)

    var q = [ ["select * from test", []] ]
    var r = query(d, q)
    println(r)
}
